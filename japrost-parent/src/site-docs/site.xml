<?xml version="1.0" encoding="ISO-8859-1"?>
<project
  xmlns="http://maven.apache.org/DECORATION/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
  name="JaProSt">
  <!-- site-docs site.xml -->
  
  <version position="bottom" />
  <publishDate
    position="right"
    format="yyyy-MM-dd HH:mm" />
  <bannerLeft>
    <name>JaProSt</name>
    <src>images/JaProSt-logo.gif</src>
    <href>${japrost.project.url}</href>
  </bannerLeft>

  <body>
    <links>
      <item
        name="Impressum"
        href="${japrost.project.url}/imprint.html" />
    </links>
    <breadcrumbs>
      <item
        name="JaProSt"
        href="${japrost.project.url}/index.html" />
      <item
        name="parent"
        href="index.html" />
    </breadcrumbs>

    <menu name="JaProSt">
      <item
        name="Übersicht"
        href="index.html" />
    </menu>

    <menu name="Projekt Aufbau">
      <item
        name="Hintergründe"
        collapse="true"
        href="background/index.html">
        <item
          name="Über JaProSt Parent"
          href="background/about.html" />
        <item
          name="Build"
          href="background/build.html" />
        <item
          name="Site"
          href="background/site.html" />
        <item
          name="Changes"
          href="changes-report.html" />
      </item>
    </menu>

    <menu
      ref="reports"
      inherit="bottom" />

  </body>
  <skin>
    <groupId>de.japrost</groupId>
    <artifactId>japrost-skin</artifactId>
    <version>${japrost.site.skin.version}</version>
  </skin>
</project>

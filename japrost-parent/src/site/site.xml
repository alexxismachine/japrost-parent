<?xml version="1.0" encoding="ISO-8859-1"?>
<project
  xmlns="http://maven.apache.org/DECORATION/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
  name="JaProSt">
  <!-- parent site.xml -->

  <version position="bottom" />
  <publishDate
    position="right"
    format="yyyy-MM-dd HH:mm" />
  <bannerLeft>
    <name>JaProSt</name>
    <src>http://www.japrost.de/images/JaProSt-logo.gif</src>
    <href>http://www.japrost.de</href>
  </bannerLeft>

  <body>
    <links>
      <item
        name="Impressum"
        href="http://www.japrost.de/imprint.html" />
    </links>
    <!-- Breadcrumbs that every project will inherit -->
    <breadcrumbs>
      <item
        name="JaProSt"
        href="http://www.japrost.de/index.html" />
    </breadcrumbs>

    <menu
      ref="reports"
      inherit="bottom" />
  </body>
  <skin>
    <groupId>de.japrost</groupId>
    <artifactId>japrost-skin</artifactId>
    <version>${japrost.site.skin.version}</version>
  </skin>
</project>
<?xml version="1.0" encoding="ISO-8859-1"?>
<project
	xmlns="http://maven.apache.org/DECORATION/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
	name="JaProSt">
	<version position="bottom" />
	<publishDate position="right" />
	<bannerLeft>
		<name>JaProSt</name>
		<src>images/JaProSt-logo.gif</src>
		<href>${japrostProjectUrl}</href>
	</bannerLeft>
	<body>

		<links>
			<item
				name="Maven"
				href="http://maven.apache.org/" />
		</links>

		<!-- Breadcrumbs that every project will inherit -->
		<breadcrumbs>
			<item
				name="JaProSt"
				href="${japrostProjectUrl}" />
		</breadcrumbs>

		<menu name="Projekt Aufbau">
			<item
				name="Hintergründe"
				collapse="true"
				href="background/index.html">
				<item
					name="Über JaProSt Parent"
					href="background/about.html" />
				<item
					name="Änderungen"
					href="background/changes.html" />
				<item
					name="Abhängikeiten"
					href="background/matrix.html" />
				<item
					name="Skin"
					href="${japrostTopicPathSkin}" />
			</item>
			<item
				name="Imprint"
				href="imprint.html" />
		</menu>

		<menu
			ref="reports"
			inherit="bottom" />

	</body>
	<skin>
		<groupId>de.japrost</groupId>
		<artifactId>${japrost.site.skin.artifact}</artifactId>
		<version>${japrost.site.skin.version}</version>
	</skin>
</project>
